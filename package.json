      "packages/babel",
      "packages/webpack",
      "packages/web"
    ]
  },
  "engines": {
    "node": ">18.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "server": "yarn workspace @movies-catalog/server start",
    "web": "yarn workspace @movies-catalog/web dev",
    "start": "concurrently --kill-others \"yarn workspace @movies-catalog/web dev\" \"yarn workspace @movies-catalog/server start\""
  },
  "husky": {
    "hooks": {
      "commit-mgs": "commit -E HUSKY_GIT_PARAMS"
    }
  },
  "relay": {
    "src": "./packages/web/src/",
    "schema": "./schema.graphql",
    "language": "typescript",
    "eagerEsModules": true,
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ]
  }
